{
  "name": "meui",
  "version": "1.0.0",
  "homepage": "https://joelhenrique.com/",
  "main": "dist/js/meui.js",
  "module": "dist/js/meui.es.js",
  "style": "dist/css/meui.css",
  "sass": "scss/meui.scss",
  "files": [
    "dist",
    "lib"
  ],
  "repository": "https://github.com/joelhenrique2000/meui.git",
  "author": "Joel Henrique Silva Santos <joelhenrique2000@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build:watch": "run-s clean && run-p build:javascript:watch build:css:watch",
    "build": "run-s clean && run-p build:javascript css",
    "build:javascript": "rollup --config build/rollup.config.js",
    "css": "npm-run-all css-compile css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/ -o dist/css/",
    "css-prefix": "npm-run-all --parallel css-prefix-*",
    "css-lint": "npm-run-all --continue-on-error --parallel css-lint-*",
    "css-lint-stylelint": "stylelint \"**/*.{css,scss}\" --cache --cache-location .cache/.stylelintcache",
    "css-minify": "cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/meui.min.css dist/css/meui.css",
    "css-prefix-main": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "js": "npm-run-all js-compile js-minify",
    "js-compile": "npm-run-all --parallel js-compile-*",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-standalone-esm": "rollup --environment ESM:true,BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config build/rollup.config.js --sourcemap",
    "js-lint": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives .",
    "js-minify": "npm-run-all --parallel js-minify-*",
    "js-minify-standalone": "terser --compress --mangle --comments \"/^!/\" --source-map \"content=dist/js/meui.js.map,includeSources,url=meui.min.js.map\" --output dist/js/meui.min.js dist/js/meui.js",
    "js-minify-standalone-esm": "terser --compress --mangle --comments \"/^!/\" --source-map \"content=dist/js/meui.esm.js.map,includeSources,url=meui.esm.min.js.map\" --output dist/js/meui.esm.min.js dist/js/meui.esm.js",
    "js-minify-bundle": "terser --compress --mangle --comments \"/^!/\" --source-map \"content=dist/js/meui.bundle.js.map,includeSources,url=meui.bundle.min.js.map\" --output dist/js/meui.bundle.min.js dist/js/meui.bundle.js",
    "js-test": "npm-run-all --parallel js-test-karma js-test-integration",
    "js-debug": "cross-env DEBUG=true npm run js-test-karma",
    "js-test-karma": "karma start js/tests/karma.conf.js",
    "js-test-integration": "rollup --config js/tests/integration/rollup.bundle.js && rollup --config js/tests/integration/rollup.bundle-modularity.js",
    "js-test-cloud": "cross-env BROWSER=true npm run js-test-karma",
    "dist": "npm-run-all --parallel css js",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css-main": "nodemon --watch scss/ --ext scss --exec \"npm-run-all css-compile css-prefix\"",
    "watch-css": "nodemon --watch scss/ --ext scss --exec \"npm-run-all css\"",
    "2watc2h-c2ss-main": "nodemon --watch scss/ --ext scss --exec \"npm-run-all css-lint css-compile css-prefix\"",
    "watch-js-main": "nodemon --watch js/src/ --ext js --exec \"npm-run-all js-compile\"",
    "2watc2h-j2s-main": "nodemon --watch js/src/ --ext js --exec \"npm-run-all js-lint js-compile\""
  },
  "peerDependencies": {
    "@popperjs/core": "^2.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@popperjs/core": "^2.4.2",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "autoprefixer": "^9.8.4",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-xo": "^0.31.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-unicorn": "^20.1.0",
    "find-unused-sass-variables": "^2.0.0",
    "lockfile-lint": "^4.3.7",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.1",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-svgo": "^1.1.0",
    "stylelint": "^13.6.1",
    "stylelint-config-twbs-bootstrap": "^2.0.3"
  },
  "dependencies": {
    "rfs": "^9.0.3",
    "terser": "^4.8.0"
  }
}
